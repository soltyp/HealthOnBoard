<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="HealthOnBoard.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:HealthOnBoard"
    Shell.FlyoutBehavior="Disabled"
    Title="HealthOnBoard">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:LoginPage}"
        Route="MainPage" />
    <ShellContent Title="Dashboard" ContentTemplate="{DataTemplate local:DashboardPage}" Route="DashboardPage" />

</Shell>
